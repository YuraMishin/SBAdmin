# Project Variables
name ?= SBAdmin

check-env:
	make --version
	echo
	git --version
	echo
	dotnet --version
	echo
	echo ${ASPNETCORE_ENVIRONMENT}
	echo
	dotnet ef
	echo
	node -v
	echo
	npm -v

check-dev:
	echo ${ASPNETCORE_ENVIRONMENT}

run:
	dotnet run --project ./src/$(name).Web --launch-profile "Production"

build:
	dotnet build
